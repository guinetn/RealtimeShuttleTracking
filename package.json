{
  "name": "realtimeshuttletracking",
  "version": "1.0.0",
  "description": "Track position of your next transportation",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .cache && rm -rf dist",
    "cleanwin": "rmdir /S /Q .cache && rmdir /S /Q dist",
    "serve": "standard && parcel src/index.html --open",    
    "prebuild": "rm -rf dist",
    "build": "parcel build src/index.html --no-source-maps --public-url ./",
    "buildwin": "rmdir /S /Q dist && parcel build src/index.html --no-source-maps --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guinetn/RealtimeShuttleTracking.git"
  },
  "keywords": [
    "tracking",
    "position",
    "suez",
    "shuttle"
  ],
  "author": "Nicolas Guinet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guinetn/RealtimeShuttleTracking/issues"
  },
  "homepage": "https://github.com/guinetn/RealtimeShuttleTracking#readme",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "marked": "^0.7.0",
    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {}
}
